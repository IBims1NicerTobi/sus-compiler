
module named_ports : int in_port -> int out_port, int out_port_two {
	out_port = in_port
	out_port_two = in_port
}

module port_names : int i -> int o, int o2 {
	named_ports np

	o, o2 = np(i)

	// Documentation!
	o, o2 = named_ports(i)

	np.in_port = i

	//np.out_port = i

	o = np.out_port
	o2 = np.out_port_two
}

module func_call : int i -> int o, int o2 {
	o, o2 = named_ports(i)
}
